{
  "name": "iglu.react.dev.env",
  "version": "1.0.0",
  "description": "Node.js Development environment at Iglu.net to develop React Components-based Web Apps with TypeScript language and Standard style. CSS Modules & PostCSS.",
  "main": "index.js",
  "scripts": {
    "build": "concurrently \"npm run server\" \"webpack --env.prod -p\"",
    "prebuild": "npm run clean:build",
    "postbuild:dev": "opn http://localhost:8080",
    "build:dev": "concurrently \"npm run server\" \"webpack-dev-server --env.dev\"",
    "clean:files": "rm -f ./build/manifest.* ./build/vendor.map",
    "clean:build": "rimraf build",
    "copy:ico": "cpy ./src/favicon.ico build",
    "deploy": "git subtree push --prefix build origin gh-pages && npm run show:deploy",
    "show:deploy": "opn https://miksansegundo.github.io/iglu.angular1.dev.env",
    "start": "",
    "server": "json-server ./test/db.json",
    "debug": "node-nightly --inspect --debug-brk node_modules/.bin/webpack-dev-server --env.dev --env.debug",
    "debug:dev": "npm run debug -- --env.dev",
    "debug:prod": "npm run debug -- --env.prod",
    "test": "karma start && echo 'Good Job :)' || echo 'Keep working :)'",
    "watch:test": "npm test -- --auto-watch --no-single-run",
    "lint": "npm run lint:js && npm run lint:css",
    "lint:js": "standard ./src --fix --verbose | snazzy && echo 'Good Standad code :)' || echo 'You can improve the Style :)'",
    "lint:css": "stylelint ./src/**/*.css && echo 'Good Style code :)' || echo 'You can improve the Style :)'"
  },
  "engines": {
    "node": "6.7",
    "npm": ">=3"
  },
  "keywords": [],
  "author": "miguelsansegundo@gmail.com",
  "license": "ISC",
  "devDependencies": {
    "@types/node": "^6.0.48",
    "@types/react": "^0.14.48",
    "@types/react-dom": "^0.14.18",
    "autoprefixer": "^6.5.1",
    "awesome-typescript-loader": "^2.2.4",
    "chai": "^3.5.0",
    "concurrently": "^3.1.0",
    "cpy-cli": "^1.0.1",
    "css-loader": "~0.26.1",
    "ejs-compiled-loader": "^1.1.0",
    "file-loader": "^0.9.0",
    "html-loader": "0.4.4",
    "html-minify-loader": "^1.1.0",
    "html-webpack-plugin": "~2.24.1",
    "humps": "~2.0.0",
    "icon-maker-loader": "^0.1.7",
    "iconfont-webpack-plugin": "0.0.9",
    "img-loader": "1.3.1",
    "inline-manifest-webpack-plugin": "^3.0.1",
    "jimp": "0.2.27",
    "jsdom": "^9.8.3",
    "json-loader": "^0.5.4",
    "json-server": "^0.9.4",
    "mocha": "^3.1.2",
    "offline-plugin": "^4.5.3",
    "opn-cli": "^3.1.0",
    "picturefill": "^3.0.2",
    "postcss": "~5.2.4",
    "postcss-autoreset": "^1.2.1",
    "postcss-browser-reporter": "^0.5.0",
    "postcss-cssnext": "~2.9.0",
    "postcss-loader": "~1.2.0",
    "postcss-reporter": "~2.0.0",
    "postcss-smart-import": "^0.6.0",
    "postcss-url": "^5.1.2",
    "progress-bar-webpack-plugin": "^1.9.0",
    "raw-loader": "^0.5.1",
    "responsive-loader": "0.7.0",
    "rimraf": "^2.5.4",
    "script-ext-html-webpack-plugin": "^1.3.2",
    "snazzy": "^5.0.0",
    "source-map-loader": "^0.1.5",
    "standard": "^8.5.0",
    "style-loader": "~0.13.1",
    "stylelint": "^7.4.2",
    "stylelint-config-css-modules": "^0.1.0",
    "stylelint-config-standard": "~15.0.0",
    "typescript": "^2.0.10",
    "url-loader": "0.5.7",
    "watchjs": "0.0.0",
    "webpack": "^2.1.0-beta.27",
    "webpack-browser-plugin": "^1.0.12",
    "webpack-config-utils": "^2.1.0",
    "webpack-dev-middleware": "~1.8.3",
    "webpack-dev-server": "^1.16.2",
    "webpack-hot-middleware": "~2.13.2",
    "webpack-md5-hash": "0.0.5",
    "webpack-notifier": "^1.4.1",
    "webpack-shell-plugin": "^0.4.3",
    "webpack-validator": "^2.2.9",
    "yargs": "^6.3.0"
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "react": "^15.3.2",
    "react-dom": "^15.3.2"
  },
  "standard": {
    "ignore": [
      "build"
    ],
    "parser": "babel-eslint"
  }
}
